<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab2.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
         <Button fx:id="btnMain" layoutX="30.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnBtnAction" text="Kliknij mnie!" textAlignment="CENTER">
            <font>
               <Font name="Comic Sans MS Bold" size="20.0" />
            </font>
         </Button>
         <TextField fx:id="text1" layoutX="26.0" layoutY="75.0" />
      </children>
   </AnchorPane>
</VBox>
